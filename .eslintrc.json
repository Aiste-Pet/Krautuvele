{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {"import/order": 
  [
      1, 
      { "groups": 
          [
            "external", 
            "builtin", 
            "internal", 
            "sibling", 
            "parent", 
            "index"
          ], 
      } 
    ] },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
